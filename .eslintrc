{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "env": {
      "browser": true,
      "node": true
  },
  "ecmaFeatures": {
      "modules": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-debugger": 0,
    "no-console": 1,
    "no-use-before-define": 2,
    "quotes": [1, "single"],
    "no-unused-vars": 1,
    "jsx-quotes": [1, "prefer-single"],
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "indent": [1, 2],
    "semi": [1, "always"]
  }
}